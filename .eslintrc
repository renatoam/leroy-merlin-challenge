{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react",
    "jest"
  ],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "generator-star-spacing": ["error", {
      "before": false,
      "after": true
    }],
    "space-before-function-paren": ["error", {"anonymous": "always", "named": "never", "asyncArrow": "always"}]
  }
}
